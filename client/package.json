{
  "name": "client",
  "version": "0.12.0",
  "description": "Woleet.ID Server web client",
  "scripts": {
    "postinstall": "node patch.js",
    "ng": "npx ng",
    "dev": "npx ng serve --port 4220 -o --ssl --disable-host-check --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:prod": "npx ng serve --port 4220 -o --configuration production --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "build": "npx ng build",
    "build:prod": "npx ng build --configuration production",
    "build:prod:silent": "npx ng build --configuration production --no-progress",
    "lint": "npx ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.3.11",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/router": "^13.3.11",
    "bs58check": "2.1.2",
    "deep-copy": "1.4.2",
    "deep-object-diff": "1.1.0",
    "libphonenumber-js": "1.9.43",
    "loglevel": "1.8.0",
    "rxjs": "6.6.7",
    "timestring": "6.0.0",
    "traverse": "0.6.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.8",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "^13.3.8",
    "@angular/compiler-cli": "^13.3.11",
    "@angular/language-service": "^13.3.11",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "core-js": "2.6.12",
    "eslint": "8.17.0",
    "tslint": "6.1.3",
    "typescript": "4.6.4"
  }
}
