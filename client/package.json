{
  "name": "client",
  "version": "0.10.0",
  "description": "Woleet.ID Server web client",
  "scripts": {
    "postinstall": "node patch.js",
    "ng": "npx ng",
    "dev": "npx ng serve --port 4220 -o --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:prod": "npx ng serve --port 4220 -o --prod --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "build": "npx ng build",
    "build:prod": "npx ng build --prod",
    "build:prod:silent": "npx ng build --prod --no-progress",
    "lint": "npx ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.0.0",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "bs58check": "2.1.2",
    "deep-copy": "1.4.2",
    "deep-object-diff": "1.1.0",
    "hammerjs": "2.0.8",
    "libphonenumber-js": "1.9.6",
    "loglevel": "1.7.1",
    "rxjs": "6.6.3",
    "timestring": "6.0.0",
    "traverse": "0.6.6",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular/cli": "^11.0.0",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@types/loglevel": "1.6.3",
    "@types/node": "14.14.14",
    "@types/traverse": "0.6.32",
    "codelyzer": "6.0.1",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "core-js": "2.6.12",
    "typescript": "4.0.5"
  }
}
