{
  "name": "client",
  "version": "0.10.0",
  "description": "Woleet.ID Server web client",
  "scripts": {
    "postinstall": "node patch.js",
    "ng": "npx ng",
    "dev": "npx ng serve --port 4220 -o --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:prod": "npx ng serve --port 4220 -o --prod --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:aot": "npx ng serve --port 4220 -o --aot --optimization --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "build": "npx ng build",
    "build:prod": "npx ng build --prod",
    "build:prod:silent": "npx ng build --prod --no-progress",
    "lint": "npx ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "bs58check": "^2.1.2",
    "deep-copy": "^1.4.2",
    "deep-object-diff": "^1.1.0",
    "hammerjs": "^2.0.8",
    "libphonenumber-js": "^1.8.5",
    "loglevel": "^1.7.0",
    "rxjs": "^6.6.3",
    "timestring": "^6.0.0",
    "traverse": "^0.6.6",
    "zone.js": "^0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.10",
    "@angular/cli": "^7.3.10",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@types/loglevel": "^1.6.3",
    "@types/node": "^14.14.5",
    "@types/traverse": "^0.6.32",
    "codelyzer": "^6.0.1",
    "ts-node": "^9.0.0",
    "tslint": "^5.20.1",
    "typescript": "3.2.4"
  }
}
