{
  "name": "client",
  "version": "0.12.0",
  "description": "Woleet.ID Server web client",
  "scripts": {
    "postinstall": "node patch.js",
    "ng": "npx ng",
    "dev": "npx ng serve --port 4220 -o --ssl --disable-host-check --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:prod": "npx ng serve --port 4220 -o --configuration production --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "build": "npx ng build",
    "build:prod": "npx ng build --configuration production",
    "build:prod:silent": "npx ng build --configuration production --no-progress",
    "lint": "npx ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "bs58check": "2.1.2",
    "deep-copy": "1.4.2",
    "deep-object-diff": "1.1.0",
    "libphonenumber-js": "1.9.43",
    "loglevel": "1.8.0",
    "rxjs": "6.6.7",
    "timestring": "6.0.0",
    "traverse": "0.6.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.17",
    "@angular/cli": "^12.2.17",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/language-service": "^12.2.16",
    "codelyzer": "6.0.2",
    "core-js": "2.6.12",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  }
}