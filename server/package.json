{
  "name": "server",
  "version": "0.11.1",
  "description": "Woleet.ID Server Node.js server",
  "scripts": {
    "setAPIVersion": "WOLEET_ID_SERVER_API_VERSION=$(cat ../swagger.yaml | grep 'version' | grep -oE '([[:digit:]]+.?)+')",
    "lint": "npx tslint src/**/*.ts",
    "check": "npx tsc --noEmit",
    "transpile": "npx tsc",
    "build": "npx tsc && cp -a assets/ dist/assets/",
    "start": "node dist/src",
    "dev": "npm run setAPIVersion; WOLEET_ID_SERVER_PRODUCTION=false DEBUG=id* npx ts-node-dev --files src/index.ts",
    "test": "npm run setAPIVersion; WOLEET_ID_SERVER_PRODUCTION=false npx mocha -r ts-node/register test/**/*.ts --exit --timeout 10000"
  },
  "private": true,
  "devDependencies": {
    "@types/koa": "2.11.8",
    "@types/mocha": "8.2.1",
    "@types/node": "14.14.32",
    "@types/sequelize": "4.28.9",
    "@types/supertest": "2.0.10",
    "mocha": "8.3.1",
    "supertest": "6.1.3",
    "ts-node": "6.2.0",
    "ts-node-dev": "1.1.6",
    "tslint": "6.1.3",
    "typescript": "4.0.7"
  },
  "dependencies": {
    "@woleet/woleet-secure-module": "1.4.0",
    "basic-auth": "2.0.1",
    "chalk": "4.1.2",
    "ioredis": "4.27.8",
    "joi": "14.3.1",
    "jose": "^3.15.4",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-bodyparser": "4.3.0",
    "koa-cors": "0.0.16",
    "koa-ejs": "^4.3.0",
    "koa-morgan": "1.0.1",
    "koa-router": "9.4.0",
    "koa-send": "5.0.1",
    "loglevel": "1.7.1",
    "lru-cache": "6.0.0",
    "mustache": "4.1.0",
    "node-cache": "5.1.2",
    "nodemailer": "6.4.18",
    "oidc-provider": "7.9.0",
    "openid-client": "4.7.4",
    "pg": "8.4.2",
    "pg-hstore": "2.3.3",
    "read": "1.0.7",
    "redlock": "4.2.0",
    "rxjs": "6.6.6",
    "sequelize": "6.6.5",
    "timestring": "6.0.0"
  }
}
