{
  "name": "server",
  "version": "0.8.0",
  "description": "Woleet.ID Server Node.js server",
  "main": "index.js",
  "scripts": {
    "lint": "npx tslint src/**/*.ts",
    "check": "npx tsc --noEmit",
    "transpile": "npx tsc",
    "build": "npx tsc && cp -a assets/ dist/server/assets/",
    "start": "node dist/server/src",
    "dev": "WOLEET_ID_SERVER_PRODUCTION=false DEBUG=id* npx ts-node-dev src/index.ts",
    "test": "WOLEET_ID_SERVER_PRODUCTION=false npx mocha --require ts-node/register test/*.ts --exit --timeout 10000"
  },
  "private": true,
  "devDependencies": {
    "@types/basic-auth": "^1.1.2",
    "@types/debug": "^0.0.31",
    "@types/http-errors": "^1.6.3",
    "@types/joi": "^13.6.3",
    "@types/koa": "^2.11.0",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-router": "7.0.37",
    "@types/loglevel": "^1.6.3",
    "@types/lru-cache": "^4.1.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.13",
    "@types/pg": "^7.14.1",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "mocha": "^5.2.0",
    "supertest": "^3.4.2",
    "ts-node": "^6.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "@woleet/woleet-secure-module": "^1.1.0",
    "basic-auth": "^2.0.1",
    "chalk": "^1.1.3",
    "joi": "^13.7.0",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "loglevel": "^1.6.6",
    "lru-cache": "^4.1.5",
    "mustache": "^3.2.1",
    "nodemailer": "^5.1.1",
    "oidc-provider": "^5.5.6",
    "openid-client": "^2.5.0",
    "pg": "^7.17.0",
    "pg-hstore": "^2.3.3",
